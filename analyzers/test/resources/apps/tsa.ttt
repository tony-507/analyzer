// Perform basic stream analysis

input = #InputReader_1;

alias(file, f);
alias(output, o);

if $f;
    input.Source = _SOURCE_FILE;
    input.FileInput.Fname = $f;
end;

input.SkipCnt = $skipCnt | 0;
input.MaxInCnt = $maxInCnt | 0;

demuxer = #TsDemuxer_1;
demuxer.Mode = $demuxMode | _DEMUX_FULL;

dh = #DataHandler_1;

link(input, demuxer);
link(demuxer, dh);